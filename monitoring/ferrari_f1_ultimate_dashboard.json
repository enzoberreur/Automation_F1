            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "orange", "value": 60},
                {"color": "yellow", "value": 75},
                {"color": "green", "value": 85},
                {"color": "blue", "value": 95}
